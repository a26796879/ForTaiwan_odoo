<odoo>
    <record id="news_crawler_action" model="ir.actions.act_window">
      <field name="name">news_crawler</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">news_crawler</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="config_token_action" model="ir.actions.act_window">
      <field name="name">config_token</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">config_token</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="TSP news" id="line-notify.menu_root"/>
    <menuitem id="menu_news_crawler_view" name="news_crawler" parent="line-notify.menu_root" action="news_crawler_action" sequence="1"/>
    <menuitem id="menu_config_token_view" name="config_token" parent="line-notify.menu_root" action="config_token_action" sequence="2"/>
</odoo>